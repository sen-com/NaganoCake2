<div class="continer">
  <div class="row mt-4">
    <div class="col-3"></div>
    <div class="col-6">
        <strong class="">会員一覧</strong>
        <table class="table my-3">
            <thead class="thead-light">
                <tr>
                <th>会員ID</th>
                <th scope="col">氏名</th>
                <th scope="col">メールアドレス</th>
                <th scope="col">ステータス</th>
                </tr>
            </thead>
            <tbody>
            <% @end_users.each do |end_user| %>
                <tr>
                <th scope="row">
                  <%= end_user.id %>
                </th>
                <td>
                  <%= link_to admin_end_user_path(end_user) do %>
                    <%= end_user.first_name %><%= end_user.last_name %>
                  <% end %>
                </td>
                <td>
                 <%= end_user.email %>
                </td>
                <td>
                  <% if end_user.is_working? %>
                   有効
                  <% else %>
                   退会済み
                  <% end %>
                </td>
                </tr>
            <% end %>
            </tbody>
        </table>
    </div>
    <div class="col-3"></div>   
  </div>
</div>